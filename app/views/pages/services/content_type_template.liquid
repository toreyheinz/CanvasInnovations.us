---
title: Service Template Page
content_type: services
---
{% extends 'layouts/application' %}
{% block main %}
  <h1>{{ service.title }}</h1>
  {{ service.description }}
  <h2>Projects</h2>
  <ul>
    {% for project in service.projects %}
      <li><a href="{% path_to project %}">{{ project.title }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}

